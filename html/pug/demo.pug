mixin Tour-Item-round()
  .tour-iteam-round
    .tour-img
      img.tour-imag(src='https://images.unsplash.com/photo-1709656541505-36bc9c102434?w=500&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxlZGl0b3JpYWwtZmVlZHwyNHx8fGVufDB8fHx8fA%3D%3D', alt='unsplash')
      a.tour-button(href='https://unsplash.com/fr', target='_blank', rel='noopener noreferrer') must see
    .tour-conten
      .tour-time Sep 28 10:00 AM - 1:00 PM
      .tour-title Chatuchak Weekend
      .tour-Evaluate
        img.tour-start(src='../imag/star.png', alt 'start')
        .tour-review 4.8 (12 reviews)
mixin heading-item-round()
  .heading-round
      .heading 
        img.heading_imag-2(src="https://images.unsplash.com/photo-1709833226085-bef3dc6c7e9e?w=500&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxlZGl0b3JpYWwtZmVlZHwyfHx8ZW58MHx8fHx8", alt="")
        .heading-content
          .heding-comment 
            h7.content-notes BESTFINALTIONS
            h3.content-title A Guide To Rocky Mountain Vacations 
          img.heading-content-imag1(src="data:image/jpeg;base64,", alt="lỗi anime") 
          img.heading-content-imag2(src="data:image/jpeg;base64,", alt="anime 2")
          img.heading-content-imag3(src="data:image/jpeg;base64,", alt="anime 3")

  

